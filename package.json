{
  "name": "postcss-bem-linter",
  "version": "2.7.1",
  "description": "A BEM linter for postcss",
  "files": [
    "index.js",
    "lib"
  ],
  "dependencies": {
    "minimatch": "^3.0.3",
    "postcss": "^6.0.6",
    "postcss-resolve-nested-selector": "^0.1.1"
  },
  "devDependencies": {
    "eslint": "1.7.3",
    "jest": "^20.0.4"
  },
  "scripts": {
    "lint": "eslint .",
    "start": "jest --watch",
    "test": "jest",
    "posttest": "npm run lint"
  },
  "jest": {
    "testRegex": "test/.*\\.js",
    "testPathIgnorePatterns": [
      "/node_modules/",
      "test-util"
    ]
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git://github.com/postcss/postcss-bem-linter.git"
  },
  "keywords": [
    "css",
    "postcss",
    "bem",
    "lint",
    "test"
  ]
}
